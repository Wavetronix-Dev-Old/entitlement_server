<tr>
  <td>
    <%= link_to user_path(user), class: "delete", data: { confirm: %(Are you sure you want to delete "#{user.name}?" This cannot be undone.) }, method: :delete, title: %(Delete "#{user.name}") do %>
      <span class="fa fa-trash-o"></span>
    <% end if current_user != user %>
    <%= user.id %>
  </td>
  <td>
    <%= user.name %>
  </td>
  <td>
    <%= user.email %>
  </td>
  <td>
    <%= user.company %>
  </td>
  <td>
    <%= user.job_title %>
  </td>
  <td>
    role goes here
  </td>
  <td>
    <%= link_to edit_identity_path(user.uid), title: %(Edit "#{user.name}") do %>
      <span class="fa fa-edit"></span>
    <% end %>
  </td>
</tr>
